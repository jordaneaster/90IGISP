    "ioredis": "^5.6.1",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.15.0",
    "pg": "^8.7.1",
    "redis": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-json-reporter": "^1.2.2",
    "jest-junit": "^16.0.0",
