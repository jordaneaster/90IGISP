# Server
PORT=3000

# JWT (90Auth)
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=1h

# PostgreSQL/PostGIS
PGHOST=localhost
PGPORT=5432
PGDATABASE=gisdb
PGUSER=postgres
PGPASSWORD=password

# Redis (90Scan)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=90igisp
KAFKA_GROUP_ID=90igisp-group

# Supabase (replaces direct PostgreSQL access)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
